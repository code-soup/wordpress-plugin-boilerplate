{
    "name": "wordpress-plugin-boilerplate",
    "version": "1.0.0",
    "license": "GPL-3.0",
    "keywords": [
        "wppb",
        "wordpress",
        "wordpress plugin",
        "wordpress plugin boilerplate"
    ],
    "description": "WordPress plugin boilerplate",
    "repository": {
        "type": "git",
        "url": "https://github.com/code-soup/wordpress-plugin-boilerplate.git"
    },
    "bugs": {
        "url": "https://github.com/code-soup/wordpress-plugin-boilerplate/issues"
    },
    "engines": {
        "node": ">= 12.16.0"
    },
    "authors": [
        {
            "homepage": "https://www.bobz.co",
            "name": "Vlado Bosnjak",
            "email": "vb@bobz.co"
        },
        {
            "homepage": "https://www.codesoup.co",
            "name": "Code Soup",
            "email": "hi@codesoup.co"
        }
    ],
    "browserslist": [
        "> 1%",
        "last 3 versions",
        "android 4",
        "safari 10",
        "not ie <= 8"
    ],
    "scripts": {
        "start": "webpack serve --hot --mode development --color --config src/webpack/config.webpack.js",
        "build": "webpack --mode development --progress --config src/webpack/config.webpack.js",
        "build:prod": "webpack --mode production --progress --config src/webpack/config.webpack.js",
        "lint": "npm run -s lint:scripts && npm run -s lint:styles",
        "lint:scripts": "eslint src/scripts src/webpack",
        "lint:styles": "stylelint \"src/styles/**/*.{css,sass,scss}\"",
        "reinit": "rimraf dist && rimraf node_modules && yarn",
        "clean": "rimraf dist"
    },
    "devDependencies": {
        "@babel/core": "^7.15.8",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/preset-env": "^7.15.8",
        "babel-loader": "^8.2.3",
        "breakpoint-sass": "^2.7.1",
        "clean-webpack-plugin": "^4.0.0",
        "css-loader": "^6.4.0",
        "cssnano": "^5.0.8",
        "eslint": "^8.0.1",
        "eslint-plugin-import": "^2.25.2",
        "eslint-webpack-plugin": "^3.0.1",
        "image-minimizer-webpack-plugin": "^2.2.0",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-jpegtran": "^7.0.0",
        "imagemin-optipng": "^8.0.0",
        "imagemin-svgo": "^10.0.0",
        "import-glob": "^1.5.0",
        "jquery": "^3.6.0",
        "mini-css-extract-plugin": "^2.4.2",
        "postcss": "^8.3.10",
        "postcss-loader": "^6.2.0",
        "postcss-preset-env": "^6.7.0",
        "resolve-url-loader": "^4.0.0",
        "rimraf": "^3.0.2",
        "sass": "^1.43.2",
        "sass-loader": "^12.2.0",
        "style-loader": "^3.3.0",
        "stylelint": "^13.13.1",
        "stylelint-config-property-sort-order-smacss": "^7.1.0",
        "stylelint-order": "^4.1.0",
        "stylelint-webpack-plugin": "^3.0.1",
        "svg-spritemap-webpack-plugin": "^4.3.3",
        "vue": "^2.6.14",
        "vue-loader": "^15.9.8",
        "vue-resource": "^1.5.3",
        "vue-template-compiler": "^2.6.14",
        "webpack": "^5.59.1",
        "webpack-assets-manifest": "^5.0.6",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.3.1",
        "webpack-merge": "^5.8.0",
        "yargs": "~17.2.1"
    }
}
